@model IEnumerable<VUserProfileViewModel>

@{
    ViewData["Title"] = "UserProfile";
}

<h1>Details</h1>
<div class="row">
    <div class="col-md-4">
        <table>
            @foreach (var user in Model)
            {
            <tr>
                <td>#</td>
                <td>
                    @user.FullName
                </td>
                <td>
                    @user.Direction
                </td>
                <td>
                    @user.Education
                </td>
                <td>
                    @user.StartDate
                </td>
                <td>
                    @user.Age
                </td>
            </tr>
            }
        </table>
    </div>
</div>
